async function startProgram() {
	playMatrixAnimation(0, true);
	await roll(0, 30, 9);
	clearMatrix();
	setSpeed(42);
	await spin(720, 10);
	stopRoll();
	clearMatrix();
	playMatrixAnimation(0, true);
	await roll(180, 24, 5);
	await roll(0, 24, 2.25);
	await delay(0.15);
	await roll(326, 48, 1.5);
	setSpeed(77);
	await spin(360, 6.5);
	stopRoll();
	setSpeed(77);
	await spin(180, 3.5);
	stopRoll();
	// fix this
	await spin(143, 1);
	await roll(getHeading(), 24, 7);
	stopRoll();
	// test
	await spin(-60, 1);
	await delay(22);
	await spin(100, 0.5);
	await roll(270, 24, 3);
	stopRoll();
	await roll(173, 24, 3.5);
	stopRoll();
	await roll(90, 24, 3.5);
	stopRoll();
	await roll(0, 24, 3.5);
	stopRoll();
	await roll(180, 24, 3.5);
	stopRoll();
	await delay(2.5);
	await spin(90, 1);
	stopRoll();
	await delay(2);
	await spin(90, 1);
	stopRoll();
	await roll(0, 24, 6);
}

